msgstr "--mkfsoptions med --noformat har ingen effekt."

#: ../pykickstart/commands/raid.py:514 ../pykickstart/commands/logvol.py:572
#: ../pykickstart/commands/partition.py:510
msgid "--mkfsoptions and --fsprofile cannot be used together."
msgstr "--mkfsoptions och --fsprofile kan inte användas tillsammans."

#: ../pykickstart/commands/device.py:98
msgid "device command requires two arguments: module type and name"
msgstr "Kommandot device behöver två argument: modultyp och namn"

#: ../pykickstart/commands/device.py:124
#, python-format
msgid "%s command requires a single argument: %s"
msgstr "Kommandot %s behöver ett ensamt argument: %s"

#: ../pykickstart/commands/device.py:133
#, python-format
